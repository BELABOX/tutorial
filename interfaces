# interfaces(5) file used by ifup(8) and ifdown(8)
# Include files from /etc/network/interfaces.d:
source-directory /etc/network/interfaces.d

# usb0 is normally the L4T network bridge over micro USB
#
# trying to get a DHCP lease on it will make the system hang
# at boot time for a few minutes
#
# only enable this if you've disabled that feature with
# systemctl disable nv-l4t-usb-device-mode.service
#
#auto usb0
#iface usb0 inet dhcp

auto usb1
iface usb1 inet dhcp

auto usb2
iface usb2 inet dhcp

auto usb3
iface usb3 inet dhcp

auto usb4
iface usb4 inet dhcp

auto eth0
iface eth0 inet dhcp

auto eth1
iface eth1 inet dhcp

auto eth2
iface eth2 inet dhcp

auto eth3
iface eth3 inet dhcp

auto eth4
iface eth4 inet dhcp
